<!doctype html>
<html lang="en">
	<head>

		<meta charset="utf-8">
		<!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge"><![endif]-->
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"/>
		<title>{{title}}</title>

		<meta name="description" content="{{description}}">
		<meta name="keywords" content="{{keywords}}">
		<meta name="author" content="devchat.tv">

		<link rel="shortcut icon" href="/images/favicon.ico">
		<link rel="apple-touch-icon" href="/images/apple-touch-icon.jpg">
		<link rel="apple-touch-icon" sizes="72x72" href="/images/apple-touch-icon-72x72.jpg">
		<link rel="apple-touch-icon" sizes="114x114" href="/images/apple-touch-icon-114x114.jpg">

		<link rel="stylesheet" type="text/css" href="/css/conferences/custom-animations.css"/>
		<link rel="stylesheet" type="text/css" href="/css/conferences/style.css"/>

		<!--[if lt IE 9]>
		<script src="/js/conferences/html5shiv.js"></script>
		<script src="/js/conferences/respond.min.js"></script>
	<![endif]-->
	</head>

	<body>
		<div class="preloader-mask">
			<div class="preloader"></div>
		</div>

		<section id="hero" class="hero-section bg1 bg-cover window-height light-text" style="background-image:url('{{hero_image}}');">
			<ul class="socials-nav">
				<li class="socials-nav-item">
					<a href="https://twitter.com/{{twitter}}">
						<span class="fa fa-twitter"></span></a>
				</li>
				<li class="socials-nav-item">
					<a href="#">
						<span class="fa fa-facebook"></span></a>
				</li>
				<li class="socials-nav-item">
					<a href="#">
						<span class="fa fa-instagram"></span></a>
				</li>
			</ul>
			<div class="heading-block centered-block align-center">
				<div class="container">
					<h5 class="heading-alt" style="margin-bottom: 8px;">
						<span class="fa fa-calendar-o base-clr-txt"></span>{{start_date | date}}
						{% if start_date !== end_date %}- {{end_date | date}}
					{% endif %}
				</h5>
				<h1 class="extra-heading">{{title}}</h1>
				<h6 class="thin base-font">Massive online conference about <strong>{{topic}}</strong>
				</h6>
				<div class="btns-container">
					<a href="{{registration_url}}" class="btn btn-md">GET TICKETS</a>
					<a href="{{cfp_url}}" class="btn btn-md">SUBMIT A TALK</a>
				</div>
			</div>
		</div>
	</section>

	<header class="header header-black">
		<div class="header-wrapper">
			<div class="container">
				<div class="col-sm-2 col-xs-12 navigation-header">
					<a href="#">
						<img loading="lazy"  src="{{logo}}" alt="js remote conf" height="34">
					</a>
					<button class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navigation" aria-expanded="false" aria-controls="navigation">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
				</div>

				<div class="col-sm-10 col-xs-12 navigation-container">
					<div id="navigation" class="navbar-collapse collapse">
						<ul class="navigation-list pull-left light-text">
							<li class="navigation-item">
								<a href="#speakers" class="navigation-link">Speakers</a>
							</li>
							<li class="navigation-item">
								<a href="#schedule" class="navigation-link">Schedule</a>
							</li>
							<li class="navigation-item">
								<a href="#prices" class="navigation-link">Prices</a>
							</li>
							<li class="navigation-item">
								<a href="#sponsors" class="navigation-link">Sponsors</a>
							</li>
							<li class="navigation-item">
								<a href="#contacts" class="navigation-link">Contact</a>
							</li>
						</ul>

						<a href="{{registration_url}}" class="pull-right buy-btn">Buy Tickets</a>
					</div>
				</div>
			</div>
		</div>
	</header>

	<section id="about" class="section align-center">
		<div class="container">
			<span class="icon section-icon icon-multimedia-12"></span>
			<h3>Front-End and Center</h3>
			<p class="text-alt">Front row seats, no travel, no stress, and <span class="highlight">no COVID-19</span>.</p>
			<br/>
			<br/>

			<div class="tabs-wrapper tabs-horizontal">
				<ul class="nav nav-tabs">
					<li class="active">
						<a href="#horizontal_tab1" data-toggle="tab">
							<h6 class="heading-alt">
								<span class="fa fa-code"></span> General info</h6>
						</a>
					</li>
					<li>
						<a href="#horizontal_tab2" data-toggle="tab">
							<h6 class="heading-alt">
								<span class="fa fa-rocket"></span> Workshops</h6>
						</a>
					</li>
					<li>
						<a href="#horizontal_tab3" data-toggle="tab">
							<h6 class="heading-alt">
								<span class="fa fa-external-link"></span> Sponsor info</h6>
						</a>
					</li>
				</ul>

				<div class="tab-content">
					<div id="horizontal_tab1" class="tab-pane fade active in">
						<div class="col-sm-5 img-column">
							<img loading="lazy"  src="/images/conferences/workfromhome-small.jpg" alt="" class="img-responsive"/>
						</div>
						<div class="col-sm-7 align-left">
							<h6>About {{title}}</h6>
							<p>{{long_description}}
								<p>
									<p>Ticket includes:</p>
									<ul>
										<li>Access to all sessions live.</li>
										<li>Discord server.</li>
										<li>Session recordings after the conference.</li>
										<li>No dress code. Come in your pajamas!</li>
									</ul>
								</div>
							</div>

							<div id="horizontal_tab2" class="tab-pane fade">
								<div class="col-sm-7 align-right">
									<h6>Workshops</h6>
									<p>Coming Soon.</p>
								</div>

								<div class="col-sm-5 img-column">
									<img loading="lazy"  src="/images/conferences/workshops.jpg" alt="" class="img-responsive"/>
								</div>
							</div>

							<div id="horizontal_tab3" class="tab-pane fade">
								<div class="col-sm-5 img-column">
									<img loading="lazy"  src="/images/conferences/heart_coffee-small.jpg" alt="" class="img-responsive"/>
								</div>
								<div class="col-sm-7 align-left">
									<h6>Sponsors apply</h6>
									<p>Introduce yourself to our audience. Let us help you get them to know and love you.</p>
									<p>
										<a href="{{sponsor_prospectus}}" class="btn btn-md">WANT TO BE A SPONSOR?</a>
									</p>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>

			<section id="counters" class="section align-center overlay bg-cover bg5 light-text" style="background-image:url('{{hero_image}}');">
				<div class="container">

					<div class="row counters-wrapper">
						<div class="col-sm-3">
							<div class="counter-block counter-block-no-border">
								<div class="counter-box">
									<div class="counter-content">
										<span class="count" data-from="0" data-to="24">0</span>

										<p class="title">speakers</p>
									</div>
								</div>
							</div>
						</div>

						<div class="col-sm-3">
							<div class="counter-block counter-block-no-border">
								<div class="counter-box">
									<div class="counter-content">
										<span class="count" data-from="0" data-to="3">0</span>

										<p class="title">days</p>
									</div>
								</div>
							</div>
						</div>

						<div class="col-sm-3">
							<div class="counter-block counter-block-no-border">
								<div class="counter-box">
									<div class="counter-content">
										<span class="count" data-from="0" data-to="3">0</span>

										<p class="title">roundtables</p>
									</div>
								</div>
							</div>
						</div>

						<div class="col-sm-3">
							<div class="counter-block counter-block-no-border">
								<div class="counter-box">
									<div class="counter-content">
										<span class="count" data-from="0" data-to="24">0</span>

										<p class="title">talks</p>
									</div>
								</div>
							</div>
						</div>
					</div>

				</div>
			</section>

			<section id="speakers" class="section align-center">
				<div class="container">
					<span class="icon section-icon icon-faces-users-04"></span>
					<h3>Speakers</h3>
					<p class="text-alt">Meet our <span class="highlight">experts</span></p>
					<br/>
					<br/>

					{% for speaker in my_speakers %}
						<div class="col-sm-4">
							<div class="speaker">
								<div class="photo-wrapper rounded"><img loading="lazy"  src="/images/conferences/speakers/{{ speakers[speaker].photo }}" alt="john doe" class="img-responsive"></div>
								<h3 class="name">{{ speakers[speaker].name }}</h3>
								<p class="text-alt">
									<small>{{ speakers[speaker].byline }}</small>
								</p>
								<p class="about">{{ speakers[speaker].bio }}</p>
								<ul class="speaker-socials">
									{% if speakers[speaker].facebook %}
										<li>
											<a href="{{ speakers[speaker].facebook }}">
												<span class="fa fa-facebook"></span></a>
										</li>
									{% endif %}
									{% if speakers[speaker].twitter %}
										<li>
											<a href="{{ speakers[speaker].twitter }}">
												<span class="fa fa-twitter"></span></a>
										</li>
									{% endif %}
									{% if speakers[speaker].github %}
										<li>
											<a href="{{ speakers[speaker].github }}">
												<span class="fa fa-github"></span></a>
										</li>
									{% endif %}
								</ul>
							</div>
						</div>
					{% endfor %}

					<div class="col-sm-12 align-center">
						<a href="{{cfp_url}}" target="_blank" class="btn btn-outline-clr btn-md">SUBMIT A TALK</a><br>
				CFP deadline is {{cfp_ends}}.
			</div>

				</div>
			</section>

			<section id="schedule" class="section schedule-section align-center">
				<div class="container">
					<span class="icon section-icon icon-office-21"></span>
					<h3>Schedule</h3>
					<p class="text-alt">Come listen to our amazing experts<br/> and <span class="highlight">join other activities</span></p>
					<br/>
					<br/>

					<!-- Schedule start -->
					<div class="schedule">

						<!-- Navigation by day start -->
						<ul class="nav nav-schedule">
							{% for day in daykeys %}
								<li{% if loop.index == 1 %} class="active"{% endif %}>
									<a href="#{{day}}" data-toggle="tab">
										<h5 class="highlight">Day {{loop.index}}</h5>
										<p class="text-alt">{{days[day]}}</p>
									</a>
								</li>
							{% endfor %}
						</ul>
						<!-- Navigation by day end -->

						<!-- First level content start -->
						<div class="tab-content">
							{% for day in daykeys %}

								<!-- Day 1 content start -->
								<div id="{{day}}" class="tab-pane fade {% if loop.index == 1 %}active {% endif %}in">

									<!-- Navigation by auditorium start -->
									<ul class="nav nav-schedule">
										<li class="active">
											<a href="#{{day}}_auditorium1" data-toggle="tab">{{hosted_on}}</a>
										</li>
									</ul>
									<!-- Navigation by auditorium start -->

									<!-- Second level content start -->
									<div class="tab-content tab-content-schedule">

										<!-- Auditorium 1 content start -->
										<div id="{{day}}_auditorium1" class="tab-pane fade active in">

											<!-- Accordion start -->
											<div class="panel-group" id="{{day}}_auditorium1_timeline">
												{% for sessionkey in my_sessions[day] %}
													{% set session = sessions[sessionkey] %}
													<!-- Lecture start -->
													<div class="panel schedule-item">
														<div class="lecture-icon-wrapper">
															<img src="/images/conferences/speakers/{{speakers[session.speaker].photo}}" alt="" class="img-responsive">
														</div>
														<a data-toggle="collapse" data-parent="#{{day}}_auditorium1_timeline" href="#{{day}}_auditorium1_time{{loop.index}}" class="schedule-item-toggle">
															<strong class="time highlight">
																<span class="icon icon-office-24"></span>{{session.start_time}}</strong>
															<h6 class="title">{{session.title}}
																<i class="icon icon-arrows-06"></i>
															</h6>
														</a>
														<div id="{{day}}_auditorium1_time{{loop.index}}" class="panel-collapse collapse in schedule-item-body">
															<article>
																<p class="description">{{session.description}}</p>
																<strong class="highlight speaker-name">{{speakers[session.speaker].name}}</strong>
															</article>
														</div>
													</div>
													<!-- Lecture end -->
												{% endfor %}
											</div>
											<!-- Accordion end -->

										</div>
										<!-- Auditorium 1 content end -->

									</div>
									<!-- Second level content end -->

								</div>
								<!-- Day 1 content end -->
							{% endfor %}
						</div>
						<!-- First level content end -->

					</div>
					<!-- Schedule end -->

				</div>

			</section>

			<section id="sign_up" class="section bg-cover overlay bg4 light-text align-center">
				<div class="container">
					<h2>
						<span class="highlight">Newsletter</span> Sign Up</h2>
					<small>No Spam - Only latest news, price and activity updates</small>
					<div class="_form_{{email_list_id}}"></div>
					<script src="https://devchat.activehosted.com/f/embed.php?id={{email_list_id}}" type="text/javascript" charset="utf-8"></script>
				</div>
			</section>
			<section id="prices" class="section align-center">
				<div class="container">
					<span class="icon section-icon icon-shopping-04"></span>
					<h3>Tickets</h3>
					<p class="text-alt">Get yours <span class="highlight">today</span></p>
					<br/>
					<br/>

					<div class="row">
						<div class="col-sm-4 col-sm-offset-4">
							<div class="package-column">
								<h6 class="package-title">Live Conference</h6>
								<div class="package-price">
									<span class="currency">$</span>75
								</div>
								<div class="package-detail">
									<ul>
										<li>
											<span class="fa fa-check check-icon"></span>Live Participation</li>
										<li>
											<span class="fa fa-check check-icon"></span>Online RoundTable Chats</li>
										<li>
											<span class="fa fa-check check-icon"></span>Talk Recordings</li>
										<li>
											<span class="fa fa-check check-icon"></span>Discord Server</li>
										<li>
											<span class="fa fa-check check-icon"></span>COVID-19 Free Virtual Handshakes</li>
										<li>
											<span class="fa fa-check check-icon"></span>Online Meet and Greet Session</li>
									</ul>
								</div>
								<a href="{{registration_url}}" class="btn btn-lg btn-outline-clr">Buy now</a>
							</div>
						</div>
					</div>
				</div>
			</section>

